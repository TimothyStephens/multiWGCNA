citHeader("To cite multiWGCNA in publications use:")

citEntry(
  entry    = "Article",
  title    = "multiWGCNA: an R package for deep mining gene co-expression networks in multi-trait expression data",
  author   = personList( as.person("Dario Tommasini"),
                         as.person("Brent L. Fogel")),
  journal  = "BMC Bioinformatics",
  year     = 2023,
  volume   = 24,
  number   = 1,
  pages    = 115,
  url      = NA,
  textVersion = paste("Tommasini, D, Fogel, BL (2023). multiWGCNA: an R package for deep mining gene co-expression networks in multi-trait expression data. BMC Bioinformatics, 24, 1:115."

  )
)

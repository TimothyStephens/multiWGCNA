citHeader("To cite multiWGCNA in publications use:")

citEntry(
  entry    = "Article",
  title    = "mutliWGCNA: an R package for deep mining gene co-expression networks in multi-trait expression data",
  author   = personList( as.person("Dario Tommasini"),
                         as.person("Brent L. Fogel")),
  journal  = "BMC Bioinformatics",
  year     = 2022,
  volume   = NA,
  number   = NA,
  pages    = NA,
  url      = NA,
  textVersion = paste("Tommasini, D., Fogel, L.B.,", 
    "mutliWGCNA: an R package for deep mining gene co-expression networks in multi-trait expression data",
    "BMC Bioinformatics forthcoming (2022)"

  )
)
